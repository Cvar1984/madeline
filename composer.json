{
  "name": "cvar1984/madeline",
  "description": "telegram bot",
  "type": "application",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Cvar1984",
      "email": "gedzsarjuncomuniti@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "danog/madelineproto": "^7",
    "cvar1984/fortune": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Cvar1984\\": "src/",
      "App\\": "src/"
    },
    "files": [
      "config/settings.php",
      "config/command.php"
    ]
  },
  "scripts": {
      "cache:clean": "find ./var/cache -type f -not -name .gitignore -delete",
      "logs:clean": "find ./var/logs -type f -not -name .gitignore -delete",
      "assets:clean": "find ./assets -type f -not -name .gitignore -delete"
  },
  "config": {
    "allow-plugins": {
      "phabel/phabel": true
    }
  }
}
